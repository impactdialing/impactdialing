<%= javascript_include_tag 'nested_form' %>
<% content_for :javascripts do %>
    <script typ ="text/javascript">
      $(function() {
        $('#script_questions_attributes_0_possible_responses_attributes_0_value').val('No response');
      });
    </script>
<% end %>
<h1>New script</h1>
<% if @script.errors.any? %>
  <div class = 'callout alert clearfix'>
    <%= error_messages_for :script %>
  </div>
<% end %>

<%= form_for @script do |builder| %>
  <%= builder.hidden_field :robo %>
  <%= render "script_name", f: builder %>  
  <%= render "script_voter_fields", f: builder, voter_fields: @voter_fields, voter_field_values: @voter_field_values %>  
  <%= render "script_text", f: builder %>
  <%= render "script_call_results", f: builder %>  
  
  <div class= "buttons">
    <button type="submit">Save</button>
  </div>
  
  

<% end %>
